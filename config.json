  "copy": {
    ".htaccess": {
      "src": "./node_modules/apache-server-configs/dist/.htaccess",
      "dst": "./public/",
      "name": ".htaccess",
      "footer": "\n<IfModule mod_rewrite.c>\n    # Rewrite example.com â†’ www.example.com\n    RewriteEngine On\n    RewriteCond %{HTTP_HOST} ^mespil.local [NC]\n    RewriteRule ^ %{ENV:PROTO}://www.%{HTTP_HOST}%{REQUEST_URI} [QSA,L,R=301]\n\n    # Handle Authorization Header\n    RewriteCond %{HTTP:Authorization} .\n    RewriteRule .* - [E=HTTP_AUTHORIZATION:%{HTTP:Authorization}]\n\n    # Redirect Trailing Slashes If Not A Folder...\n    RewriteCond %{REQUEST_FILENAME} !-d\n    RewriteCond %{REQUEST_URI} (.+)/$\n    RewriteRule ^ %1 [L,R=301]\n\n    # Handle Front Controller...\n    RewriteCond %{REQUEST_FILENAME} !-d\n    RewriteCond %{REQUEST_FILENAME} !-f\n    RewriteRule ^ index.php [L]\n</IfModule>",
      "replace": {
        "(Options -MultiViews)": "<IfModule mod_negotiation.c>\n    $1\n</IfModule>",
        "# (<IfModule mod_rewrite\\.c>\n)#(    RewriteEngine On\n)#(    RewriteCond %\\{HTTPS\\} !=on\n)#(    RewriteRule \\^ https:\/\/%\\{HTTP_HOST\\}%\\{REQUEST_URI\\} \\[R=301,L\\]\n)#( <\/IfModule>)": "$1$2$3$4$5",
        "(<IfModule mod_rewrite\\.c>\n)(    RewriteEngine On\n)(    # \\(1\\)\n)(    # RewriteCond %\\{HTTPS\\} !=on\n)(    RewriteCond %\\{HTTP_HOST\\} \\^www\\\\.\\(\\.\\+\\)\\$ \\[NC\\]\n)(    RewriteRule \\^ %\\{ENV:PROTO\\}:\/\/%1%\\{REQUEST_URI\\} \\[R=301,L\\]\n)(<\/IfModule>)": "# $1# $2# $3# $4# $5# $6# $7",
        "# (<IfModule mod_rewrite\\.c>\n)# (    RewriteEngine On\n)# (    RewriteCond %\\{REQUEST_FILENAME\\} !-f\n)# (    RewriteRule \\^\\(\\.\\+\\)\\\\.\\(\\\\d\\+\\)\\\\.\\(bmp\\|css\\|cur\\|gif\\|ico\\|jpe\\?g\\|m\\?js\\|png\\|svgz\\?\\|webp\\|webmanifest)(\\)\\$ \\$1\\.\\$3 \\[L\\]\n)# (<\/IfModule>)": "$1$2$3$4|mp4|ogv|webm|flv|avi|swf|json$5$6"
      }
    },
    "bootstrap.scss": {
      "src": "./node_modules/bootstrap/scss/bootstrap.scss",
      "dst": "./resources/_/assets/sass/vendor/",
      "name": "_bootstrap.scss",
      "replace": {
        "^(@import \"(code|grid|custom-forms|badge|jumbotron)\";)$": "// $1",
        "^@import \"": "@import \"./node_modules/bootstrap/scss/"
      }
    },
    "sky-photoswipe.scss": {
      "src": [
        "./node_modules/photoswipe/src/css/_main-settings.scss",
        "./node_modules/photoswipe/src/css/main.scss",
        "./node_modules/photoswipe/src/css/default-skin/default-skin.scss"
      ],
      "dst": "./resources/sky/assets/sass/vendor/",
      "name": "photoswipe.scss",
      "header": "@import '../functions';\n@import '../variables';\n",
      "replace": {
        "^(@import \"(../)?main-settings\";)$": "// $1",
        "^\\$pswp__assets-path: ''": "$pswp__assets-path: '/css/' + $localPath + '/vendor/photoswipe/'"
      }
    },
    "sky-photoswipe-icons": {
